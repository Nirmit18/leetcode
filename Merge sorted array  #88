class Solution {
public:
    void merge(vector<int>& nums1, int m, vector<int>& nums2, int n) {
        int i,j;
        vector<int>v;
        for(i=0;i<m;i++){
            //cout<<nums1[i];
           v.push_back(nums1[i]);
        }
        
        for(j=0;j<n;j++){
           // cout<<nums2[j]<<endl;
            v.push_back(nums2[j]);
        }
        sort(v.begin(),v.end());
        // for(i=0;i<v.size();i++){
        //     cout<<v[i];
        nums1=v;

        }
    
};
